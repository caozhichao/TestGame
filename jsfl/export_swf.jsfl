//main();function swfOneFile(filePath){	//fl.trace(filePath)	if(filePath == undefined){		filePath = fl.browseForFileURL("open","select file");	}	fl.openDocument(filePath);	var dom = fl.getDocumentDOM();	var profileXML = dom.exportPublishProfileString('Default'); 	var lowXml = profileXML.toLowerCase();	var findString = "<flashfilename>";	//var findString = "<flashFileName>";	var startIndex = lowXml.indexOf(findString) + findString.length;	findString = "</flashfilename>";	//findString = "</flashFileName>";	var endIndex = lowXml.indexOf(findString);	var curName = profileXML.substring(startIndex, endIndex);	var pubPath = curName.replace("assets","assets_tw");	//var pubPath = "../bin/" + dom.name;  	profileXML = profileXML.replace(curName, pubPath);  	profileXML = profileXML.replace("<html>1</html>", "<html>0</html>");  	dom.importPublishProfileString(profileXML);  	//fl.trace(curName);	//fl.trace(profileXML);	dom.publish();	//dom.exportSWF("a.swf", true);	dom.close(false);}function publishSwf(){	fl.outputPanel.clear();			//var filePath = fl.browseForFileURL("open","select file");			//return;		var folderPath = fl.browseForFolderURL("open","select file");		fl.trace(folderPath);		var fileList= FLfile.listFolder(folderPath,"files");		//fl.trace(fileList);		for each(var fName in fileList){			//fl.trace(fName);			//fl.trace(fName.lastIndexOf(".fla"));			if(fName.lastIndexOf(".fla")!=-1){				//fl.trace(fName+"====");				swfOneFile(folderPath+"/"+fName);				//fl.openDocument(folderPath+"/"+fName);				//var dom = fl.getDocumentDOM();								//dom.publish();				//dom.close(false);				//fl.closeDocument(dom);			}			//fl.trace(fName);		}		//fl.openDocument(filePath);		//dom = fl.getDocumentDOM();}//fl.outputPanel.clear();//openFile();fl.outputPanel.clear();publishSwf();//swfOneFile();fl.outputPanel.clear();